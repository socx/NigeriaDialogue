							    @*<select id="regioncode" data-rel="chosen">*@
                                <select id="region" >
							        <option value="">ALL</option>
							    </select>
						    </div>
						</div>
                    </div>
                    <div class="span6">
                    </div>
                </form>
            </div>
            <div class="row-fluid showgrid">
               <div class="span6">
		            <style>
		                #map-canvas { position: relative; }
